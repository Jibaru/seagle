		"clsx": "^2.1.1",
		"lucide-react": "^0.541.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@vitejs/plugin-react": "^2.0.1",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.4.17",
		"typescript": "^4.6.4",
		"vite": "^3.0.7"
	}
