{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check": "biome check --write --unsafe --config-path=biome.json",
		"lint": "biome lint --fix --unsafe --config-path=biome.json",
